<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wenkey">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TMP"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:replace="android:supportsRtl">
        <activity android:name=".sections.Home.MainActivity.view.MainActivity"></activity>
        <activity android:name=".sections.Profile.SettingActivity.view.SettingActivity" />
        <activity android:name=".sections.Profile.EditInfoActivity.view.EditInfoActivity" />
        <activity android:name=".sections.Messages.MessageActivity.view.MessageActivity" />
        <activity android:name=".sections.Profile.EducationActivity.view.EducationActivity" />
        <activity android:name=".sections.Profile.WorkActivity.view.WorkActivity" />
        <activity android:name=".sections.Login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sections.Signup.Signup"></activity>
    </application>

</manifest>